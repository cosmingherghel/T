--CAPDEV-174 BVX Mistery Bonus

WITH CONTACTS AS (
SELECT PPCD.ACCOUNT_NUMBER, CAMPAIGN_NAME
FROM HDM.CAMPAIGN_CONTACT_FACT CCF
JOIN HDM.AUDIENCE_DIM AD ON CCF.AUDIENCE_KEY = AD.AUDIENCE_KEY
JOIN HDM.CAMPAIGN_DIM CD ON CCF.CAMPAIGN_KEY = CD.CAMPAIGN_KEY
JOIN HDM.PLAYER_PROFILE_CURRENT_DIM PPCD ON CCF.PLAYER_PROFILE_CURRENT_KEY = PPCD.PLAYER_PROFILE_CURRENT_KEY
WHERE CAMPAIGN_NAME LIKE '%CAPDEV174 AUDIENCE%' and CONTACT_DATE >= SYSDATE - INTERVAL '28' DAY
) 
--TO GET PLAYERS WITH BAD EMAILS
,BAD_EMAILS AS (
SELECT  DISTINCT
PPCD.ACCOUNT_NUMBER
FROM HDM.EMAIL_EVENT_FACT EEF
LEFT OUTER JOIN HDM.PLAYER_PROFILE_CURRENT_DIM PPCD ON LOWER(TRIM(PPCD.EMAIL)) = LOWER(TRIM(EEF.DESTINATION_EMAIL))
WHERE 1=1
	AND (TRIM(EEF.CATEGORY) IN ('0', '2', '8')
	OR (TRIM(EEF.CATEGORY) = '1' AND EEF.BOUNCE_TYPE IN ('1007', '1003') AND UPPER(EEF.BOUNCE) REGEXP_LIKE '(SENDER|IP ADDRESS|BLOCKED)')
	OR (TRIM(EEF.CATEGORY) = '9' AND TRIM(UPPER(EEF.BOUNCE)) = 'COMPLAINT'))	
)
--	
-- Start of player script 
,Base AS (
SELECT 
	PPCd.ACCOUNT_NUMBER 
	,PPCD.PROFILE_UUID 
	,KAP.PLAYER_PROFILE_CURRENT_KEY
	,TRUNC(DATE_FOR_AGGREGATION_LEVEL, 'IW') AS WeekMonday
	,CASE WHEN PLF.LAST_PLAY_DATE >  TRUNC(SYSDATE-7) THEN 'ACTIVE - OVERALL'
	      WHEN PLF.LAST_PLAY_DATE >  TRUNC(SYSDATE-28) AND PLF.LAST_PLAY_DATE &lt;= TRUNC(SYSDATE-7) THEN 'IA RISK - OVERALL'
	      WHEN PLF.LAST_PLAY_DATE >  TRUNC(SYSDATE-180) AND PLF.LAST_PLAY_DATE &lt;= TRUNC(SYSDATE-28) THEN 'IA SHORT - OVERALL'
	      WHEN PLF.LAST_PLAY_DATE >  TRUNC(SYSDATE - 365) AND PLF.LAST_PLAY_DATE &lt;= TRUNC(SYSDATE-180) THEN 'IA SEASON - OVERALL'
	      WHEN PLF.LAST_PLAY_DATE >  TRUNC(SYSDATE-730) AND PLF.LAST_PLAY_DATE &lt;= TRUNC(SYSDATE-365) THEN 'IA LONG - OVERALL'
	      WHEN PLF.LAST_PLAY_DATE &lt;= TRUNC(SYSDATE-730) THEN 'DORMANT - OVERALL'
	 END AS OVERALL_LIFESTAGE
	,sum(ZEROIFNULL(kap.RNG_HANDLE)+ZEROIFNULL(kap.LIVECASINO_HANDLE)+ZEROIFNULL(kap.SPORTS_HANDLE)) AS TOTAL_HANDLE
	,sum(TOTAL_GGR) AS TOTAL_GGR
	,SUM(TOTAL_BONUS_REDEEMED) AS TOTAL_BONUS_REDEEMED
	,CASE WHEN sum(TOTAL_GGR)=0 THEN -1 else SUM(TOTAL_BONUS_REDEEMED)/sum(TOTAL_GGR) END AS Bonus_GGR_Ratio
	,max(CASE WHEN KAP.HOSTED_YN='Y' THEN 1 ELSE 0 END ) AS IsHosted 
	,sum(NET_DEPOSIT_ADJUSTED) AS NET_DEPOSIT_ADJUSTED 
	,sum(DEPOSITS_AMOUNT_W_ADJ) AS Deposit_Amount 
--	,Kap.HOSTED_YN
FROM BI_MISC_VIEWS.HOLLY_KPI_AGGREGATED_PLAYER KAP
JOIN HDM.PLAYER_PROFILE_CURRENT_DIM PPCD ON KAP.PLAYER_PROFILE_CURRENT_KEY=PPCD.PLAYER_PROFILE_CURRENT_KEY 
JOIN Hdm.PLAYER_LIFETIME_FACT plf ON KAP.PLAYER_PROFILE_CURRENT_KEY=PLF.PLAYER_PROFILE_CURRENT_KEY
WHERE 1=1 
	AND BUSINESS_REGION_CODE='UFM'
	AND REPORTING_BRAND='BVX'
	AND DATE_FOR_AGGREGATION_LEVEL >'2024-01-01'
	AND PPCD.TEST_ACCOUNT_YN='N'
GROUP BY 1,2,3,4,5	
HAVING sum(NET_DEPOSIT_ADJUSTED)&lt;450 
)
--

,FinalTable AS (
SELECT 
	ACCOUNT_NUMBER 
	,PROFILE_UUID 
	,PLAYER_PROFILE_CURRENT_KEY
	,WeekMonday
	,TOTAL_HANDLE
	,TOTAL_GGR
	,TOTAL_BONUS_REDEEMED
	,Bonus_GGR_Ratio
	,IsHosted 
	,NET_DEPOSIT_ADJUSTED 
	,Deposit_Amount
	,CASE WHEN Deposit_Amount>=100 THEN 1 ELSE 0 END AS HasDeposit100PlusPrevWeek 
	,CASE WHEN Deposit_Amount>=100 THEN 1 ELSE 0 END AS HasAlsoWager500PlusPrevWeek
	,CASE WHEN floor(Deposit_Amount/100)>0 THEN floor(Deposit_Amount/100)*10 END  AS POINTS_Deposit
	,CASE WHEN floor(TOTAL_HANDLE/500)>0 THEN floor(TOTAL_HANDLE/500)*10 END  AS POINTS_Wager 
	,ZEROIFNULL(LOCAL.POINTS_Deposit)+ZEROIFNULL(LOCAL.POINTS_Wager) AS Total_Point 
FROM Base 
WHERE 1=1 
	AND Bonus_GGR_Ratio BETWEEN 0 AND 0.5 
	AND OVERALL_LIFESTAGE IN ('ACTIVE - OVERALL','IA RISK - OVERALL')
--	ORDER BY 1 
), 

PLAYER_LIST AS (
SELECT DISTINCT PROFILE_UUID
FROM (

---------------------------------------------------
  --EDIT CODE BELOW - MUST CONTAIN PROFILE_UUID--
---------------------------------------------------


--PULL PLAYERS IN THE MODEL
SELECT 
	ACCOUNT_NUMBER 
	,PROFILE_UUID 
	,HasDeposit100PlusPrevWeek
	,HasAlsoWager500PlusPrevWeek
--	,WeekMonday
--	,CURRENT_DATE-WeekMonday AS LastWeekCheck 
FROM FinalTable
WHERE 1=1
 AND CURRENT_DATE-WeekMonday=7 -- NEED TO CHANGE THIS FOR QA  

---------------------------------------------------
  --DO NOT EDIT THE CODE BELOW -
---------------------------------------------------

)
)

SELECT DAT.*, CD.CAMPAIGN_ID, CASE WHEN CD.CAMPAIGN_ID IS NOT NULL THEN 'UPDATE' ELSE 'CREATE' END AS PROCESS,
'${TARGETDIR}'|| DAT.AUDIENCE_NAME AS FN, BBRM.BRAND AS REGION_BRAND, BBRM.REGION_UUID, BBRM.COUNTRY AS REGION_COUNTRY
FROM (
SELECT DISTINCT
    --${audience_code} AS AUDIENCE_CODE
	444 AS AUDIENCE_CODE
    ,PREFERRED_SITE_NAME AS BRAND
    ,PPCD.PROFILE_UUID AS PLAYER_ID
    ,USERNAME
    ,ACCOUNT_NUMBER
    ,INITCAP(FIRST_NAME) AS FIRST_NAME
    ,LAST_NAME
    ,COUNTRY
    ,LANGUAGE
    ,LOWER(TRIM(EMAIL)) AS EMAIL
    ,'0' AS PHONE_NUMBER
    ,CURRENCY_CODE AS CURRENCY
 	,'${campaignName}' AS CAMPAIGN_NAME
	,'${MMTrans}' AS TRANSFORMATION
	,'${activities}' AS ACTIVITIES
	,'${audienceName}' AS AUDIENCE_NAME
	,'${campaignRunTimeOfDay}' AS RUNTIME
	,'${campaignStartDate}' AS CAMPAIGN_START_DATE
	,'${campaignEndDate}' AS CAMPAIGN_END_DATE
	,'${campaignFrequency}' AS CAMPAIGN_FREQUENCY
 FROM
        PLAYER_LIST PL
        JOIN HDM.PLAYER_PROFILE_CURRENT_DIM PPCD ON PL.PROFILE_UUID = PPCD.PROFILE_UUID
		JOIN HDM.GLOBAL_COUNTRY_DIM GCD ON GCD.GLOBAL_COUNTRY_KEY = PPCD.GLOBAL_COUNTRY_KEY
        WHERE  email not like '%-%'
		--AND GCD.BUSINESS_REGION_CODE in ('AUS')
		AND PPCD.PREFERRED_SITE_NAME='BVX'
		AND PPCD.FRAUDULENT_ACCOUNT_YN &lt;&gt;'Y' --EXCLUDE RISK OF FRAUD
		AND PPCD.TAGS NOT LIKE '%BONUS_ABUSE%' --EXCLUDE BONUS ABUSERS
		AND PPCD.EMAIL_UNSUBSCRIBED_YN &lt;&gt;'Y' --EXCLUDE OPT OUTS
		AND PPCD.ENABLED_FLAG = 1 --EXCLUDE DISABLED ACCOUNTS
		AND PPCD.STATE_CODE NOT IN ('MARYLAND','MARY LAND','MD','M.D.','NEW JERSEY','NEWJERSEY','NJ','N.J.','NEVADA','NV','N.V.','NEW YORK','NEWYORK','NY','N.Y.','DELAWARE','DE','D.E.','QC') --EXCLUDE RESTRICTED STATES
		AND PPCD.ACCOUNT_NUMBER not in (select ACCOUNT_NUMBER from BAD_EMAILS) --EXCLUDE BAD EMAILS
		--AND PPCD.SKILLED_PLAYER_YN &lt;&gt; 'Y' --EXCLUDE SKILLED PLAYERS
		AND PPCD.TEST_ACCOUNT_YN &lt;&gt; 'Y' --EXCLUDE TEST ACCOUNTS
		AND PPCD.DELETE_FLAG != 1
		AND PPCD.BONUS_EXCLUSIONS = 'Not Specified'
		AND PPCD.AFFILIATE_PAYOUT_ACCOUNT_YN != 'Y'
		
)DAT
    JOIN LMCCULLUM.BEATRIX_BC_REGION_MAPPING BBRM ON DAT.COUNTRY = BBRM.COUNTRY AND DAT.BRAND = BBRM.BRAND
    LEFT OUTER JOIN HDM.CAMPAIGN_DIM CD ON DAT.CAMPAIGN_NAME = CD.CAMPAIGN_NAME
    --LEFT OUTER JOIN CONTACTS C ON DAT.ACCOUNT_NUMBER = C.ACCOUNT_NUMBER
--WHERE C.ACCOUNT_NUMBER IS NULL
</sql>
    <limit>0</limit>
